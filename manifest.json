{
  "manifest_version": 3,
  "name": "GSV Skill Graph",
  "version": "1.2",
  "content_scripts": [{
    "matches": ["https://gsv.fun/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }, {
    "matches": ["https://p.eagate.573.jp/game/gfdm/*/p/setting/*"],
    "js": ["content_573.js"],
    "run_at": "document_idle"
  }],
  "web_accessible_resources": [{
    "resources": ["song_mapping.json"],
    "matches": ["https://gsv.fun/*"]
  }]
}