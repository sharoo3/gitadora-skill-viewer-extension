{
  "manifest_version": 3,
  "name": "GSV Extension",
  "version": "1.4",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "GSV Skill Graph"
  },
  "content_scripts": [{
    "matches": ["https://gsv.fun/*"],
    "js": ["content.js"],
    "run_at": "document_idle"
  }, {
    "matches": ["https://p.eagate.573.jp/game/gfdm/*/p/setting/*"],
    "js": ["content_573.js"],
    "run_at": "document_idle"
  }]
}
